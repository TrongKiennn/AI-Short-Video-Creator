NEXT_PUBLIC_FIREBASE_API_KEY=your_firebase_api_key

# Deployment used by `npx convex dev`
# CONVEX_DEPLOYMENT=dev:your-convex-deployment # team: your-team, project: your-project

# NEXT_PUBLIC_CONVEX_URL=https://your-convex-url.convex.cloud
CONVEX_SELF_HOSTED_URL='http://127.0.0.1:3210'
CONVEX_SELF_HOSTED_ADMIN_KEY=your_convex_self_hosted_admin_key

NEXT_PUBLIC_CONVEX_URL=http://127.0.0.1:3210
NEXT_PUBLIC_GEMINI_API_KEY=your_gemini_api_key
NEXT_PUBLIC_AIGURULAB_API_KEY=your_aigurulab_api_key
GROQ_API_KEY=your_groq_api_key

# API Keys to fetch trending news and keywords
NEWS_API_KEY=your_news_api_key
SERP_API_KEY=your_serp_api_key

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-supabase-url.supabase.co
NEXT_PUBLIC_SUPABASE_KEY=your_supabase_key

# YouTube API Configuration
# Get these from Google Cloud Console > APIs & Services > Credentials
YOUTUBE_CLIENT_ID=your_youtube_client_id_here
YOUTUBE_CLIENT_SECRET=your_youtube_client_secret_here
YOUTUBE_REDIRECT_URI=http://localhost:3000/api/youtube/auth

# You'll get this after running the auth flow once
# Visit /api/youtube/auth in your browser to get the refresh token
YOUTUBE_REFRESH_TOKEN=your_refresh_token_here